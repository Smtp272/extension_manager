{
  "manifest_version": 3,
  "version": "1.0",
  "name": "My Extensions",
  "description": "Manages your extensions",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["management","storage"]
}